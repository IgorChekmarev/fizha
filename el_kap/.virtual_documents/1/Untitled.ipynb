import matplotlib.pyplot as plt
import numpy as np

# Исходные данные (предполагается, что e и sigma уже определены)
# e = ваши данные по оси X
# sigma = ваши данные по оси Y

# Аппроксимация многочленами
# Степень 2
coeffs_2 = np.polyfit(e, sigma, 2)
poly_2 = np.poly1d(coeffs_2)

# Степень 4
coeffs_4 = np.polyfit(e, sigma, 4)
poly_4 = np.poly1d(coeffs_4)

# Создание точек для гладкой кривой
x_smooth = np.linspace(min(e), max(e), 200)
y_smooth_2 = poly_2(x_smooth)
y_smooth_4 = poly_4(x_smooth)

# Функция для форматирования уравнения красиво
def format_equation(coeffs, degree):
    terms = []
    for i, coef in enumerate(coeffs):
        power = degree - i
        if abs(coef) > 1e-10:  # Игнорируем очень маленькие коэффициенты
            if power == 0:
                terms.append(f"{coef:.4f}")
            elif power == 1:
                terms.append(f"{coef:.4f}x")
            else:
                terms.append(f"{coef:.4f}x^{power}")
    
    equation = " + ".join(terms)
    equation = equation.replace("+ -", "- ")  # Красивое отображение отрицательных членов
    return equation

# Настройка стиля
plt.style.use('seaborn-v0_8-darkgrid')  # Красивый современный стиль

# Создание фигуры с улучшенным дизайном
fig, ax = plt.subplots(figsize=(12, 8), facecolor='white')
ax.set_facecolor('#f8f9fa')  # Светло-серый фон для области графика

# Исходные точки - делаем их более заметными и красивыми
scatter = ax.scatter(e, sigma, 
                    color='#2E4057',           # Темно-синий цвет
                    s=100,                      # Размер точек
                    marker='o',                  # Круглые маркеры
                    edgecolors='white',          # Белая обводка
                    linewidth=2,                  # Толщина обводки
                    alpha=0.8,                    # Легкая прозрачность
                    label='Экспериментальные данные',
                    zorder=5)                     # Чтобы точки были сверху

# Аппроксимация степень 2 (пунктирная, тонкая)
line2, = ax.plot(x_smooth, y_smooth_2, 
                color='#E64B35',        # Красно-оранжевый
                linestyle='--',          # Пунктирная линия
                linewidth=1.5,           # Тонкая линия
                label='_nolegend_',      # Временное скрытие из легенды
                zorder=4)

# Аппроксимация степень 4 (пунктирная, тонкая)
line4, = ax.plot(x_smooth, y_smooth_4, 
                color='#4EB265',        # Зеленый
                linestyle=':',           # Точечный пунктир
                linewidth=1.5,           # Тонкая линия
                label='_nolegend_',      # Временное скрытие из легенды
                zorder=4)

# Форматирование уравнений
equation_2 = format_equation(coeffs_2, 2)
equation_4 = format_equation(coeffs_4, 4)

# Расчет R² для качества аппроксимации
y_pred_2 = poly_2(e)
y_pred_4 = poly_4(e)
ss_res_2 = np.sum((sigma - y_pred_2) ** 2)
ss_res_4 = np.sum((sigma - y_pred_4) ** 2)
ss_tot = np.sum((sigma - np.mean(sigma)) ** 2)
r2_2 = 1 - (ss_res_2 / ss_tot)
r2_4 = 1 - (ss_res_4 / ss_tot)

# Создание красивой легенды с уравнениями и R²
from matplotlib.lines import Line2D
custom_lines = [
    Line2D([0], [0], marker='o', color='w', markerfacecolor='#2E4057', 
           markersize=10, markeredgecolor='white', markeredgewidth=2, 
           label='Экспериментальные данные'),
    
    Line2D([0], [0], color='#E64B35', linestyle='--', linewidth=1.5, 
           label=f'2-я степень: y = {equation_2}\n$R^2 = {r2_2:.4f}$'),
    
    Line2D([0], [0], color='#4EB265', linestyle=':', linewidth=1.5, 
           label=f'4-я степень: $y = {equation_4}$\n$R^2 = {r2_4:.4f}$')
]

# Добавление легенды
legend = ax.legend(handles=custom_lines, 
                  loc='best',              # Автоматический выбор позиции
                  fontsize=10,
                  frameon=True,
                  fancybox=True,
                  shadow=True,
                  framealpha=0.9,
                  edgecolor='#333333')

# Настройка сетки
ax.grid(True, 
        linestyle=':',           # Точечная линия сетки
        linewidth=0.7,
        color='#cccccc',
        alpha=0.7)

# Настройка осей
ax.set_xlabel('e', fontsize=14, fontweight='bold', color='#333333', labelpad=10)
ax.set_ylabel('σ', fontsize=14, fontweight='bold', color='#333333', labelpad=10)
ax.set_title('Аппроксимация экспериментальных данных\nмногочленами 2-й и 4-й степени', 
             fontsize=16, fontweight='bold', color='#333333', pad=20)

# Настройка внешнего вида границ
ax.spines['top'].set_visible(False)      # Убираем верхнюю границу
ax.spines['right'].set_visible(False)    # Убираем правую границу
ax.spines['left'].set_color('#666666')   # Цвет левой границы
ax.spines['bottom'].set_color('#666666') # Цвет нижней границы
ax.spines['left'].set_linewidth(1)       # Толщина левой границы
ax.spines['bottom'].set_linewidth(1)     # Толщина нижней границы

# Настройка ticks (делений на осях)
ax.tick_params(axis='both', 
               which='major', 
               labelsize=11,
               color='#666666',
               width=1.5,
               length=6)

# Добавление аннотации с коэффициентами (опционально)
coeff_text_2 = f'2-я степень: y = {coeffs_2[0]:.4f}x² + {coeffs_2[1]:.4f}x + {coeffs_2[2]:.4f}'
coeff_text_4 = f'4-я степень: y = {coeffs_4[0]:.4f}x⁴ + {coeffs_4[1]:.4f}x³ + {coeffs_4[2]:.4f}x² + {coeffs_4[3]:.4f}x + {coeffs_4[4]:.4f}'

# Добавление текста с коэффициентами в нижней части графика
fig.text(0.05, 0.02, coeff_text_2, fontsize=9, color='#E64B35', 
         bbox=dict(boxstyle='round,pad=0.5', facecolor='white', alpha=0.8, edgecolor='#E64B35'))
fig.text(0.05, 0.06, coeff_text_4, fontsize=9, color='#4EB265', 
         bbox=dict(boxstyle='round,pad=0.5', facecolor='white', alpha=0.8, edgecolor='#4EB265'))

# Автоматическое выравнивание
plt.tight_layout()

# Отображение графика
plt.show()

# Вывод информации в консоль
print("=" * 60)
print("РЕЗУЛЬТАТЫ АППРОКСИМАЦИИ")
print("=" * 60)
print(f"\nМногочлен 2-й степени:")
print(f"y = {coeffs_2[0]:.6f}x² + {coeffs_2[1]:.6f}x + {coeffs_2[2]:.6f}")
print(f"R² = {r2_2:.6f}")

print(f"\nМногочлен 4-й степени:")
print(f"y = {coeffs_4[0]:.6f}x⁴ + {coeffs_4[1]:.6f}x³ + {coeffs_4[2]:.6f}x² + {coeffs_4[3]:.6f}x + {coeffs_4[4]:.6f}")
print(f"R² = {r2_4:.6f}")
